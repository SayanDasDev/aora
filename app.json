{
  "expo": {
    "name": "Aora",
    "slug": "aora",
    "scheme": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-android.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sayandasdev.aora",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-android.png",
        "backgroundColor": "#161622"
      }
    },
    "web": {
      "favicon": "./assets/logo-android.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8846bebc-2fc8-4f38-bf8c-3daa7023ead9"
      }
    }
  }
}
